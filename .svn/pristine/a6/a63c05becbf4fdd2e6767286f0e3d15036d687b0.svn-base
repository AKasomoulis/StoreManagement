		<div class="services-page main grid-wrap">

			<header class="grid col-full">
				<hr>
				<p class="fleft">
					<%
						out.print(session.getAttribute("username") + " home page");
					%>
				</p>
			</header>


			<aside class="grid col-one-quarter mq2-col-full">
				<p class="mbottom">Lorem ipsum dolor sit amet, consectetuer
					adipiscing elit. Morbi commodo, ipsum sed pharetra gravida, orci
					magna rhoncus neque, id pulvinar odio lorem non turpis.</p>
				<menu>
					<ul>
						<a href="adminGeneral.jsp">General</a>
						<br>
						<a href="<%=request.getContextPath()%>/AllStores">Stores</a>
						<br>
						<a href="<%=request.getContextPath()%>/AllUsers">Users</a>
						<br>
						<a href="<%=request.getContextPath()%>/AllProducts">Products</a>
						<br>
					</ul>
				</menu>
			</aside>

			<section class="grid col-three-quarters mq2-col-full">
				<div class="work-page main grid-wrap">

					<header class="grid col-full">
						<hr>
						<p class="fleft">Project</p>
					</header>


			<!--  		<aside class="grid col-one-quarter mq2-col-one-third mq3-col-full">-->

						<%
							UserForApproval userfa = null;
							String username = null;
							String password = null;
							String firstName = null;
							String lastName = null;
							String email = null;
							Role role = null;
							String roleName = null;
						%>

						<%
							userfa = (UserForApproval) request.getAttribute("userforapproval");
							username = userfa.getUsername();
							password = userfa.getPassword();
							firstName = userfa.getFirstName();
							lastName = userfa.getLastName();
							email = userfa.getEmail();
							role = userfa.getRole();
							roleName = role.getRoleName();
						%>



						<form action="UserForApprovalInfo" method="post">
							
							<h4>User Information (Not approved)</h4>
							</p>
							
							<figure class="grid col-one-third mq1-col-one-half mq2-col-one-third mq3-col-full work_1">
							<h5>Username</h5>
								<h6>
									<%
										out.print(username);
									%>
								</h6>
							</figure>
							
							<figure class="grid col-one-third mq1-col-one-half mq2-col-one-third mq3-col-full work_1">
							<h5>First name</h5>
								<h6>
									<%
										out.print(firstName);
									%>
								</h6>
							</figure>
							
							<figure class="grid col-one-third mq1-col-one-half mq2-col-one-third mq3-col-full work_1">
							<h5>Last name</h5>
								<h6>
									<%
										out.print(lastName);
									%>
								</h6>
							</figure>

							<figure class="grid col-one-third mq1-col-one-half mq2-col-one-third mq3-col-full work_1">
							<h5>Password</h5>
								<h6>
									<%
										out.print(password);
									%>
								</h6>
							</figure>

							<figure class="grid col-one-third mq1-col-one-half mq2-col-one-third mq3-col-full work_1">
							<h5>E-mail</h5>
								<h6>
									<%
										out.print(email);
									%>
								</h6>
							</figure>
							
							<figure class="grid col-one-third mq1-col-one-half mq2-col-one-third mq3-col-full work_1">
							<h5>Role</h5>
								<h6>
									<%
										out.print(roleName);
									%>
								</h6>
							</figure>
							</br>
						</form>


						<form action="ApproveUser" method="post">
							<p>
								<%
									request.getSession().setAttribute("notApprovedusername", username);
								%>
								<button id="approve" type="submit">Approve</button>
							</p>

						</form>

						<form action="RejectUser" method="post">
							<p>
								<%
									request.getSession().setAttribute("notApprovedusername", username);
								%>
								<button id="reject" type="submit">Reject</button>
							</p>

						</form>


<!-- 					</aside> -->




				</div>

			</section>



		</div>
