<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Signup</servlet-name>
  	<display-name>Signup</display-name>
  	<description></description>
  	<servlet-class>gr.uoa.di.storemanagement.web.Signup</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>gr.uoa.di.storemanagement.web.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AllUsers</servlet-name>
  	<display-name>AllUsers</display-name>
  	<description></description>
  	<servlet-class>gr.uoa.di.storemanagement.web.AllUsers</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AllStores</servlet-name>
  	<display-name>AllStores</display-name>
  	<description></description>
  	<servlet-class>gr.uoa.di.storemanagement.web.AllStores</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddStore</servlet-name>
  	<display-name>AddStore</display-name>
  	<description></description>
  	<servlet-class>gr.uoa.di.storemanagement.web.AddStore</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>Signup</servlet-name>
  	<url-pattern>/Signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AllUsers</servlet-name>
  	<url-pattern>/AllUsers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AllStores</servlet-name>
  	<url-pattern>/AllStores</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddStore</servlet-name>
  	<url-pattern>/AddStore</url-pattern>
  </servlet-mapping>

</web-app>
